{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "app-plus": {
          "titleNView": {
            "searchInput": {
              "align": "left",
              "backgroundColor": "#F6F7F8",
              "borderRadius": "6px",
              "disabled": true,
              "placeholder": "uni-app视频点播app&小程序项目实战",
              "placeholderColor": "#959FA0"
            },
            "buttons": [
              {
                "float": "right",
                "text": "分类",
                "fontSize": "14px",
                "color": "#959FA0",
                "colorPressed": "#333333"
              }
            ]
          }
        }
      }
    },
    {
      "path": "pages/cate/cate",
      "style": {
        "app-plus": {
          "titleNView": {
            "searchInput": {
              "align": "left",
              "backgroundColor": "#F6F7F8",
              "borderRadius": "6px",
              "disabled": true,
              "placeholder": "uni-app视频点播app&小程序项目实战",
              "placeholderColor": "#959FA0"
