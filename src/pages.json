      "style": {
        "navigationBarTitleText": "登入",
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/user-space/user-space",
      "style": {
        "navigationBarTitleText": "",
        "app-plus": {
          "titleNView": {
            "type": "transparent",
            "buttons": [
              {
                "type": "menu",
                "float": "right"
              }
            ]
          }
        }
      }
    },
    {
      "path": "pages/video-detail/video-detail",
      "style": {
        "navigationBarTextStyle": "white",
        "navigationBarTitleText": "",
        "app-plus": {
          "titleNView": false,
          "subNVues": [
            {
              "id": "top",
              "path": "pages/video-detail/subNVue/video",
              "style": {
                "position": "dock",
                "dock": "top",
                "width": "100%",
                "height": "225px"
              }
            }
          ]
        }
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "在线点播",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "borderStyle": "black",
    "selectedColor": "#FB7299",
    "color": "#444444",
    "list": [
      {
        "pagePath": "pages/index/index",
