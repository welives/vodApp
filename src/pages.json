          "titleNView": {
            "buttons": [
              {
                "color": "#333333",
                "colorPressed": "#333333",
                "float": "right",
                "fontSize": "18px",
                "fontSrc": "/static/iconfont.ttf",
                "text": "\ue61a"
              },
              {
                "color": "#333333",
                "colorPressed": "#333333",
                "float": "right",
                "fontSize": "18px",
                "fontSrc": "/static/iconfont.ttf",
                "text": "\ue695"
              }
            ]
          }
        }
      }
    },
    {
      "path": "pages/create/create",
      "style": {
        "navigationBarTitleText": "发布作品",
        "app-plus": {
          "titleNView": {
            "buttons": [
              {
                "color": "#FB7299",
                "colorPressed": "#FB85A7",
                "float": "right",
                "fontSize": "14px",
                "text": "发布"
              }
            ]
          }
        }
      }
    },
    {
      "path": "pages/user-video/user-video",
      "style": {
        "navigationBarTitleText": "我的作品"
      }
    },
    {
      "path": "pages/user-collect/user-collect",
